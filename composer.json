{
    "name": "valadanchik/blockchain-old",
    "description": "This is a laravel 6 package for interacting with blockchain api",
    "keywords": ["php", "bitcoin", "blockchain", "cryptocurrency", "Laravel","open source","laravel 5"],
    "type": "library",

    "require": {
        "valadanchik/blockchain-old": "dev-master",
        "php": "^7.2",
        "illuminate/support": "^5.5 || ^6.0",
        "symfony/css-selector": "^2.7 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Valadanchik\\Blockchain\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Volodya",
            "email": "sar.volodya@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Valadanchik\\Blockchain\\BlockchainServiceProvider"
            ],
            "aliases": {
                "Paystack": "Valadanchik\\Blockchain\\Facades\\Blockchain"
            }
        }
    }
}



